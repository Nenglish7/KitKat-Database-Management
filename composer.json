{
    "name": "obozaxy/kitkat",
    "type": "library",
    "keywords": [
        "kitkat",
        "extremely-fast",
        "secure-by-default",
        "database-management",
        "eloquent",
        "php72"
    ],
    "description": "Secure by default, extremely fast, and eloquent database management.",
    "homepage": "https://github.com/obozaxy/KitKat-Database-Management",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Nicholas English",
            "homepage": "https://github.com/Nenglish7",
            "email": "nenglish0820@outlook.com"
        },
        {
            "name": "Kit Kat Contributors",
            "homepage": "https://github.com/obozaxy/KitKat-Database-Management/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/obozaxy/KitKat-Database-Management/issues",
        "docs": "https://github.com/obozaxy/KitKat-Database-Management/tree/master/docs"
    },
    "require": {
        "php": "^7.2",
        "ext-pdo": "*"
    },
    "autoload": {
        "psr-4": {
            "Obozaxy\\KitKat\\": "./src"
        },
        "files": ["./kitkat.optimize.php"]
    },
    "suggest": {
        "ext-pdo_mysql": "To create a MySQL database connection.",
        "ext-pdo_sqlite": "To create a SQLite database connection.",
        "ext-pdo_oci": "To create an Oracle database connection.",
        "ext-pdo_pgsql": "To create a PgSQL database connection."
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "ext-xdebug": "*"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true
}
