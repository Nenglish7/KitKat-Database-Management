{
    "name": "obozaxy/kitkat",
    "type": "library",
    "keywords": [
        "kitkat",
        "extremely-fast"
        "secure-by-default",
        "database-management",
        "eloquent",
        "php72"
    ],
    "description": "Secure by default, extremely fast, and eloquent database management.",
    "homepage": "https://github.com/obozaxy/KitKat-Database-Management",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Nicholas English",
            "homepage": "https://github.com/Nenglish7",
            "email": "nenglish0820@outlook.com",
            "role": "Owner/Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "psr/container": "*",
        "psr/cache": "*",
        "psr/log": "*",
        "ext-pdo": "*"
    },
    "autoload": {
        "psr-4": {
            "Obozaxy\\Database\\": "./src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
